  b : C,A,D,B
  c : C,B,D,A
  d : B,A,C,D
}
CONFIGURATION {
  A0 B0 C0 D0
  a? b? cA d?
}
CONFIGURATION {
  A0 B0 C0 D0
  a? bB cA d?
}
CONFIGURATION {
  A0 B1 C0 D0
  a? bC cA d?
}
CONFIGURATION {
  A0 B1 C0 D0
  aB bC cA d?
}
CONFIGURATION {
